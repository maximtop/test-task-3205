{
  "name": "Chrome Extension Webpack",
  "background": {
    "page": "background.html"
  },
  "icons": {
    "128": "icon-128.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "content_scripts": [
    {
      "matches": ["https://www.instagram.com/*"],
      "js": ["content.bundle.js"]
    }
  ],
  "permissions": [
    "downloads"
  ]
}
